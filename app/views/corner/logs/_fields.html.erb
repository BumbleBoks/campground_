<%= render 'shared/error_messages', object: f.object %>

<div id="date_picker"></div>

<div>
	<%= f.label :log_date, "Date" %>
	<%= f.text_field :log_date %>
</div>	

<div>
	<%= f.label :title, "Title" %>
	<%= f.text_field :title %>
</div>	

<div>
	<%= f.label :content, "Content" %>
	<%= f.text_area :content, rows: 10 %>
</div>	
	
<div>
	<%= f.submit "Save log" %>			
</div>
